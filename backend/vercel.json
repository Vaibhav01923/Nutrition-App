{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "crons": [
    {
      "path": "/internal/recalc-weekly",
      "schedule": "5 0 * * 1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
